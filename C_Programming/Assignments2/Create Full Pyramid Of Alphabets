#include <stdio.h>

int main() {
    int n ;
     printf("ENTEAR YOUR NUMBEAR :");
     scanf("%d",&n);
    for (int i = 0; i < n; i++) {
        // Print spaces
        for (int j = n - i; j > 1; j--) {
            printf(" ");
        }
        // Print asterisks
        for (int k = 0; k <= i; k++) {
            printf(" * ");
        }
        printf("\n");
    }
    return 0;
}

